number_1 = input("Введите первое число")
number_2 = input("Введите второе число")
if number_1 < number_2:
    print(number_1 + "Меньше чем" + number_2)
else:
    print(number_1 + "Больше чем" + number_2)
if number_1 == number_2:
    print("Числа равны")
else:
    print("Числа не равны")
print("Конец первого задания 1 урока")
